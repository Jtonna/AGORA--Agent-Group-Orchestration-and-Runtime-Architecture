{
  "name": "agora-framework",
  "version": "0.1.0",
  "description": "AGORA - Agent Group Orchestration and Runtime Architecture",
  "license": "MIT",
  "author": "Jtonna",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jtonna/AGORA--Agent-Group-Orchestration-and-Runtime-Architecture.git",
    "directory": "agora"
  },
  "homepage": "https://github.com/Jtonna/AGORA--Agent-Group-Orchestration-and-Runtime-Architecture/tree/v3/agora#readme",
  "keywords": ["agora", "agent", "orchestration", "multi-agent", "ai", "llm", "framework"],
  "type": "module",
  "main": "./dist/server/app.js",
  "types": "./dist/server/app.d.ts",
  "bin": {
    "agora": "./dist/cli/index.js"
  },
  "files": ["dist/", "scaffold/", "assets/"],
  "exports": {
    ".": { "types": "./dist/server/app.d.ts", "import": "./dist/server/app.js" },
    "./client": { "types": "./dist/client/index.d.ts", "import": "./dist/client/index.js" }
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js start",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "fastify": "^5.2.1",
    "@fastify/cors": "^11.0.0",
    "commander": "^13.1.0",
    "uuid": "^11.1.0",
    "unique-names-generator": "^4.7.1",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
